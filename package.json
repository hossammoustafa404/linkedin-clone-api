    "csurf": "^1.11.0",
    "dotenv": "^16.4.5",
    "firebase-admin": "^12.0.0",
    "helmet": "^7.1.0",
    "multer": "^1.4.5-lts.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "slugify": "^1.6.6",
    "socket.io": "^4.7.4",
    "typeorm": "^0.3.19",
    "typesense": "^1.7.2"
